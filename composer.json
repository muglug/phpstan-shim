{
	"name": "muglug/test-phpstan-shim-no-replace",
	"description": "Testing PHPStan Phar distribution without replacement",
	"license": ["MIT"],
	"require": {
		"php": "~7.1"
	},
	"replace": {
		"phpstan/phpstan": "self.version",
		"phpstan/phpdoc-parser": "^0.3.3"
	},
	"bin": [
		"phpstan",
		"phpstan.phar"
	],
	"extra": {
		"branch-alias": {
			"dev-master": "0.11-dev"
		}
	},
	"autoload": {
		"files": ["bootstrap.php"]
	}
}
